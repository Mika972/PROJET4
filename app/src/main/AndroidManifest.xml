<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mike.mareu">

    <application
        android:name=".MeetingRoomApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Mareu">
        <activity android:name=".ui.meeting_room_list.AddMeetingRoomActivity"
                  android:parentActivityName=".ui.meeting_room_list.MainActivity"></activity>
        <activity android:name=".ui.meeting_room_list.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>